        "explode": {
            "body": " this.broadcast('hit',50,5);  this.children_by_name.ParticleSystem1.explode();\n    this.children_by_name.ParticleSystem2.explode();\n    if(this.random() > .5)\n        this.audioAPI.playSound(this.explodeSound, false, 600);\n    else\n        this.audioAPI.playSound(this.explodeSound2, false, 600);   ",
            "parameters": []
        },
        "tick": {
            "body": " // The tick function is called 20 times every second.\n    // Write code here to animate over time\n   this.fuse--; if(Math.floor(this.fuse) == 0)\n        this.explode();\n    if(Math.floor(this.fuse) == -130)\n      vwf.deleteNode(this.id); ",
            "parameters": []
        }
